{
  "name": "website",
  "version": "0.0.0",
  "private": true,
  "description": "https://nullvoxpopuli.com",
  "repository": "https://github.com/NullVoxPopuli/website/",
  "license": "MIT",
  "author": "NullVoxPopuli",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "prepare:safe": "cd node_modules/sharp && pnpm install && pnpm run install",
    "build": "ember build --environment=production",
    "lint": "npm-run-all --aggregate-output --continue-on-error --parallel \"lint:!(fix)\"",
    "lint:fix": "npm-run-all --aggregate-output --continue-on-error --parallel lint:*:fix",
    "lint:hbs": "ember-template-lint .",
    "lint:hbs:fix": "ember-template-lint . --fix",
    "lint:js": "eslint . --cache",
    "lint:js:fix": "eslint . --fix",
    "start": "ember serve",
    "test": "npm-run-all lint test:*",
    "test:ember": "ember test",
    "deploy": "ember build -e production && cp config/_redirects dist/"
  },
  "devDependencies": {
    "@babel/core": "7.19.6",
    "@ember/optional-features": "2.0.0",
    "@ember/test-helpers": "2.8.1",
    "babel-eslint": "10.1.0",
    "broccoli-asset-rev": "3.0.0",
    "ember-auto-import": "2.4.3",
    "ember-cli": "4.6.0",
    "ember-cli-app-version": "5.0.0",
    "ember-cli-babel": "7.26.11",
    "ember-cli-dependency-checker": "3.3.1",
    "ember-cli-fastboot": "3.3.2",
    "ember-cli-htmlbars": "6.1.1",
    "ember-cli-inject-live-reload": "2.1.0",
    "ember-cli-sri": "2.1.1",
    "ember-cli-terser": "4.0.2",
    "ember-data": "4.4.1",
    "ember-fetch": "8.1.2",
    "ember-load-initializers": "2.1.2",
    "ember-page-title": "7.0.0",
    "ember-qunit": "5.1.5",
    "ember-resolver": "8.0.3",
    "ember-shadow-dom": "2.0.1",
    "ember-showdown-prism": "3.2.0",
    "ember-source": "4.8.0",
    "ember-template-lint": "4.16.1",
    "empress-blog": "4.1.0",
    "empress-blog-casper-template": "4.3.0",
    "eslint": "7.32.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-ember": "11.1.0",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-qunit": "7.3.1",
    "loader.js": "4.7.0",
    "npm-run-all": "4.1.5",
    "prember": "1.1.1",
    "prettier": "2.7.1",
    "prismjs": "1.29.0",
    "qunit": "2.19.3",
    "qunit-dom": "2.0.0",
    "sharp": "0.31.1",
    "webpack": "5.74.0"
  },
  "engines": {
    "node": ">= 14.5.0"
  },
  "ember": {
    "edition": "octane"
  },
  "volta": {
    "node": "14.17.5",
    "yarn": "1.22.11"
  },
  "dependencies": {
    "@ember/render-modifiers": "2.0.4",
    "@glimmer/component": "1.1.2",
    "@glimmer/tracking": "1.1.2",
    "ember-prism": "0.13.0",
    "prismjs-glimmer": "1.1.1"
  },
  "pnpm": {
    "notes": {
      "rollup": "See: https://github.com/empress/empress-blog-casper-template/issues/55",
      "sharp": "Need to manually install stuff as it has post-install hooks"
    },
    "onlyBuiltDependencies": [
      "sharp"
    ],
    "overrides": {
      "sharp": "0.31.0"
    },
    "peerDependencyRules": {
      "allowAny": [
        "rollup"
      ],
      "ignoreMissing": [
        "rollup"
      ]
    }
  }
}
