{
  "name": "website",
  "version": "0.0.0",
  "private": true,
  "description": "https://nullvoxpopuli.com",
  "repository": "https://github.com/NullVoxPopuli/website/",
  "license": "MIT",
  "author": "NullVoxPopuli",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "prepare:safe": "cd node_modules/sharp && pnpm install && pnpm run install",
    "build": "ember build --environment=production",
    "lint": "npm-run-all --aggregate-output --continue-on-error --parallel \"lint:!(fix)\"",
    "lint:fix": "npm-run-all --aggregate-output --continue-on-error --parallel lint:*:fix",
    "lint:hbs": "ember-template-lint .",
    "lint:hbs:fix": "ember-template-lint . --fix",
    "lint:js": "eslint . --cache",
    "lint:js:fix": "eslint . --fix",
    "start": "ember serve",
    "test": "npm-run-all lint test:*",
    "test:ember": "ember test",
    "deploy": "ember build -e production && cp config/_redirects dist/"
  },
  "devDependencies": {
    "@babel/core": "^7.23.6",
    "@ember/optional-features": "^2.0.0",
    "@ember/test-helpers": "^3.2.1",
    "@babel/eslint-parser": "7.11.0",
    "broccoli-asset-rev": "^3.0.0",
    "ember-auto-import": "^2.7.1",
    "ember-cli": "5.6.0",
    "ember-cli-app-version": "^6.0.1",
    "ember-cli-babel": "^8.2.0",
    "ember-cli-dependency-checker": "^3.3.2",
    "ember-cli-fastboot": "^4.1.2",
    "ember-cli-htmlbars": "^6.3.0",
    "ember-cli-inject-live-reload": "^2.1.0",
    "ember-cli-sri": "^2.1.1",
    "ember-cli-terser": "^4.0.2",
    "ember-data": "~4.12.0",
    "ember-fetch": "^8.1.2",
    "ember-load-initializers": "^2.1.2",
    "ember-page-title": "^8.1.0",
    "ember-qunit": "^8.0.2",
    "ember-resolver": "^11.0.1",
    "ember-shadow-dom": "^2.0.1",
    "ember-showdown-prism": "^4.4.0",
    "ember-source": "^4.6.0",
    "ember-template-lint": "^5.13.0",
    "empress-blog": "^4.1.0",
    "empress-blog-casper-template": "4.3.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-ember": "^11.12.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^5.0.1",
    "eslint-plugin-qunit": "^8.0.1",
    "loader.js": "^4.7.0",
    "npm-run-all2": "^5.0.0",
    "prember": "^2.0.0",
    "prettier": "^3.1.1",
    "prismjs": "^1.29.0",
    "qunit": "^2.20.0",
    "qunit-dom": "^3.0.0",
    "sharp": "0.33.2",
    "webpack": "^5.89.0"
  },
  "engines": {
    "node": ">= 18.0.0"
  },
  "ember": {
    "edition": "octane"
  },
  "volta": {
    "node": "20.11.1",
    "yarn": "1.22.21",
    "pnpm": "8.15.4"
  },
  "dependencies": {
    "@ember/render-modifiers": "^2.1.0",
    "@glimmer/component": "1.1.2",
    "@glimmer/tracking": "1.1.2",
    "ember-prism": "^0.13.0",
    "prismjs-glimmer": "^1.1.1"
  },
  "pnpm": {
    "overrides": {
      "ember-prism": "^0.13.0",
      "prismjs-glimmer": "^1.1.1"
    },
    "notes": {
      "overrides": "can't trust some of our deps to have their deps up to date",
      "rollup": "See: https://github.com/empress/empress-blog-casper-template/issues/55",
      "sharp": "Need to manually install stuff as it has post-install hooks"
    },
    "onlyBuiltDependencies": [
      "sharp"
    ],
    "peerDependencyRules": {
      "allowAny": [
        "rollup"
      ],
      "ignoreMissing": [
        "rollup"
      ]
    }
  }
}
